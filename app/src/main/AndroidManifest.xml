<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devisscher.niek.projectghost" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="nl.mprog.Ghost.projectghost.gameStart"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nl.mprog.Ghost.projectghost.winScreen"
            android:label="@string/title_activity_win_screen" >
        </activity>
        <activity
            android:name="nl.mprog.Ghost.projectghost.MainActivity"
            android:label="@string/title_activity_game_start" >
        </activity>
        <activity
            android:name="nl.mprog.Ghost.projectghost.highScore"
            android:label="@string/title_activity_high_score" >
        </activity>
    </application>

</manifest>
